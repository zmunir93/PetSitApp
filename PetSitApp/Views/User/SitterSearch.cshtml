@model IQueryable<Sitter>
<h1>Sitter Search Page</h1>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <!-- Add more headers for additional sitter info -->
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var sitter in Model)
            {
                <tr>
                    <td>@sitter.FirstName</td>
                    <td>@sitter.LastName</td>
                    <!-- Display other sitter info -->
               </tr>
            }
        }
        else {
            <p>No Results Found</p>
        }
    </tbody>
</table>